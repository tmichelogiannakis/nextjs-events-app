{
  "name": "nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "commit:prehook": "npm run lint && npm run typecheck",
    "lint": "eslint src --ext .ts,.tsx,.js",
    "push:prehook": "npm test",
    "test": "jest",
    "typecheck": "tsc"
